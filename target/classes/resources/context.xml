<?xml version="1.0" encoding="UTF-8" ?>
<controllers>
    <controller class="java.Containers.Servlet.Controllers.FrontController" path="/">
        <handler method="GET">
            <handle>handleGet</handle>
        </handler>
        <handler method="POST">
            <handle>handlePost</handle>
        </handler>
    </controller>


<controller class="java.Containers.Servlet.Controllers.LoginController" path="/login">
<handler method="POST">
    <handle>doPost</handle>
</handler>
    <handler method="GET">
        <handle>doGet</handle>
    </handler>
</controller>
</controllers>

